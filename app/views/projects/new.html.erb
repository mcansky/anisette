  <div class="project">
    <div class="last_commits">

    <h4>Add a project</h4>

    <%= error_messages_for :project %>
    <% form_for :project, @project, :url => { :action => "create" } do |f| -%>
    <p><%= label_tag 'title' %><br/>
    <%= f.text_field :title %></p>

    <p><%= label_tag 'Description' %><br/>
    <%= text_area(:project, :desc, :cols => 30, :rows => 10) %>

    <p><%= submit_tag 'Report !' %></p>
    <% end -%>

    </div>
  </div>
